pipeline {
	agent any
		stages {
			stage("Checkout") {
				steps {
					git url: 'https://github.com/AvielCo/ParkNBark'
				}
			}
			stage("build") {
				    steps {
					script {
						sh 'chmod +x ./gradlew compileDebugSources'  
						}
					    }
					}
			stage(“Unit test”) {
				steps {
					sh "./gradlew test"
				}
			}

		}
}
