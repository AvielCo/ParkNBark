pipeline {
	agent any
		stages {
			stage("Checkout") {
				steps {
					git url: 'https://github.com/AvielCo/ParkNBark'
				}
			}
			stage("build") {
					    steps {
						script {
						    try {
							sh 'chmod +x gradlew MainActivity' //run a gradle task
						    } 
						}
					    }
			}

	}
}
