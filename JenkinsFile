pipeline {
	agent any
		stages {
			stage("Checkout") {
				steps {
					git url: 'https://github.com/AvielCo/ParkNBark'
				}
			}
			stage("build") {
				    steps {
					script {
						sh 'chmod +x ./gradlew app/src/main/java/com/evan/parknbark/MainActivity'  
						}
				}
			}

		}
}
